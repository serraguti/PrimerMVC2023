@model List<int>

<h1>Tabla Multiplicar simple</h1>

<form method="post">
    <label>Introduzca un número</label>
    <input type="text" name="numero"
        class="form-control"/>
    <button class="btn btn-warning">
        Tabla multiplicar
    </button>
</form>

@if (Model != null){
    <ul>
        @foreach (int num in Model)
        {
            <li>@num</li>
        }
    </ul>
}