@model List<int>

<h1>Conjetura Collatz Múltiples objetos</h1>

<form method="post">
    <label>Introduzca un número</label>
    <input type="number" name="numeroCollatz"
        class="form-control"/>
    <button class="btn btn-success">
        Mostrar datos
    </button>
</form>

@if (Model != null){
    <ul>
        @foreach (int num in Model)
        {
            <li>@num</li>
        }
    </ul>
}